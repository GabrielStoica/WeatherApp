<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label layoutX="126.0" layoutY="21.0" text="Țară:">
            <font>
                <Font name="SansSerif Bold" size="30.0" />
            </font>
        </Label>
        <Label layoutX="307.0" layoutY="21.0" text="Oraș:">
            <font>
                <Font name="SansSerif Bold" size="30.0" />
            </font>
        </Label>
        <ComboBox fx:id="country" layoutX="97.0" layoutY="60.0" onAction="#handleCountry" prefWidth="150.0" />
        <ComboBox fx:id="city" layoutX="271.0" layoutY="60.0" prefWidth="150.0" />
        <Button fx:id="showButton" layoutX="454.0" layoutY="60.0" mnemonicParsing="false" onAction="#handleShowButton" text="Afișează vremea" />
    </children>
</AnchorPane>
